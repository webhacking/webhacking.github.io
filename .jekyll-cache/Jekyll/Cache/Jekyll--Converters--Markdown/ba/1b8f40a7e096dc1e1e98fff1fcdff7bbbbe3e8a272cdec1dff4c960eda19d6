  <span class="kd">const</span> <span class="nx">currentClicked</span> <span class="o">=</span> <span class="p">(</span> <span class="nx">val</span><span class="p">.</span><span class="nx">currentTarget</span><span class="p">.</span><span class="nx">getAttribute</span><span class="p">(</span><span class="dl">'</span><span class="s1">number-of-clicked</span><span class="dl">'</span><span class="p">)</span> <span class="p">)</span> <span class="p">?</span> <span class="nx">val</span><span class="p">.</span><span class="nx">currentTarget</span><span class="p">.</span><span class="nx">getAttribute</span><span class="p">(</span><span class="dl">'</span><span class="s1">number-of-clicked</span><span class="dl">'</span><span class="p">)</span> <span class="p">:</span> <span class="mi">1</span><span class="p">;</span>
  <span class="nx">val</span><span class="p">.</span><span class="nx">currentTarget</span><span class="p">.</span><span class="nx">setAttribute</span><span class="p">(</span><span class="dl">'</span><span class="s1">number-of-clicked</span><span class="dl">'</span><span class="p">,</span> <span class="nb">parseInt</span><span class="p">(</span><span class="nx">currentClicked</span><span class="p">)</span> <span class="o">+</span> <span class="mi">1</span><span class="p">);</span>
