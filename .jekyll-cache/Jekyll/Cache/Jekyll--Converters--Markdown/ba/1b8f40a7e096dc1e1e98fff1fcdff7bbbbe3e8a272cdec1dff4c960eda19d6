<span class="kd">let</span> <span class="nx">source</span> <span class="o">=</span> <span class="dl">'</span><span class="s1">acbqibekboabkdnopenmenuckofqamdqlvopenmenuasdqweascqwekasdcnaskdeqweqwcasdopenmenu</span><span class="dl">'</span><span class="p">;</span>
