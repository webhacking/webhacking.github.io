      <span class="k">return</span> <span class="p">(</span> <span class="nx">x</span><span class="p">.</span><span class="nx">join</span><span class="p">(</span><span class="dl">''</span><span class="p">)</span> <span class="o">===</span> <span class="nx">matchSequence</span> <span class="p">)</span>
    <span class="p">}),</span>
    <span class="nx">count</span><span class="p">(),</span>
  <span class="p">)</span>
  <span class="p">.</span><span class="nx">subscribe</span><span class="p">((</span><span class="nx">val</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">{</span>
