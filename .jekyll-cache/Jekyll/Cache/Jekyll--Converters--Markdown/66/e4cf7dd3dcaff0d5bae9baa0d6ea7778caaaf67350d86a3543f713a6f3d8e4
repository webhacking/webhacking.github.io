// ì»¤ì„œë¥¼ ë‚˜íƒ€ë‚´ê±°ë‚˜ ìˆ¨ê¹€

void CursorVisible(bool blnCursorVisible) // Console.CursorVisible = false;

{

  CONSOLE_CURSOR_INFO cursorInfo;

  GetConsoleCursorInfo(GetStdHandle(STD_OUTPUT_HANDLE), &amp; cursorInfo);

  cursorInfo.bVisible = blnCursorVisible;

  SetConsoleCursorInfo(GetStdHandle(STD_OUTPUT_HANDLE), &amp; cursorInfo);

}

// ì»¤ì„œìœ„ì¹˜ë¥¼ ì„¤ì •

void SetCursorPosition(int cursorLeft, int cursorTop) //
Console.SetCursorPosition(posX, posY);

{

  int posX = cursorLeft;

  int posY = cursorTop;

  COORD pos = {
    posX,
    posY
  };

  SetConsoleCursorPosition(GetStdHandle(STD_OUTPUT_HANDLE), pos);

}

// ì»¤ì„œì˜ í˜„ì¬ ìœ„ì¹˜ì¢Œí‘œ ì–»ê¸°

COORD GetCursorPosition(void)

{

  COORD cursor;

  CONSOLE_SCREEN_BUFFER_INFO cursorInfo;

  GetConsoleScreenBufferInfo(GetStdHandle(STD_OUTPUT_HANDLE), &amp; cursorInfo);

  cursor.X = cursorInfo.dwCursorPosition.X;

  cursor.Y = cursorInfo.dwCursorPosition.Y;

  return cursor;

}

//ì‹œì‘ í™”ë©´ ë° ì½˜ì†” ì´ˆê¸°í™”

void ConsoleInit()

{

  // ì½˜ì†” ì´ˆê¸°í™” ë° ì‹œì‘ í™”ë©´ êµ¬ì„± ì˜ì—­

  printf("C Language Tetris Game\n\n");

  printf("Made by. ReverserHW\n\n");

  printf("=================================== \n");

  printf(":: ì¡°ì‘í‚¤ :: \n");

  printf("[â†’] ë¸”ë¡ì„ ì˜¤ë¥¸ìª½ìœ¼ë¡œ ì´ë™ \n");

  printf("[â†] ë¸”ë¡ì„ ì™¼ìª½ìœ¼ë¡œ ì´ë™ \n");

  printf("[â†‘] ë¸”ë¡ì„ ì™¼ìª½ìœ¼ë¡œ íšŒì „í•˜ê¸° \n");

  printf("[â†“] ë¸”ë¡ì„ ì•„ë˜ë¡œ 1ì¹¸ ë‚´ë¦¬ê¸° \n");

  printf("[Space] ì•„ë˜ë¡œ ë–¨ì–´ëœ¨ë¦¬ê¸° \n");

  printf(" \n");

  printf("ì•„ë¬´í‚¤ë‚˜ ëˆ„ë¥´ë©´ ì‹œì‘ë©ë‹ˆë‹¤. \n");

  printf("=================================== \n");

  getch();

  system("cls");

  CursorVisible(false);

  SetCursorPosition(0, 0);

}

// ê²Œì„íŒ ê·¸ë¦¬ê¸°

void DrawField(void)

{

  int x, y;

  //ì¤‘ì•™ ë³´ë“œ ë¼ì¸

  for (x = 1; x &lt;= BOARD_WIDTH + 1; x++)

  {

    board[BOARD_HEIGHT][x] = 1; //board ë°°ì—´ ì¤‘ì•™ 1ì¸ì‹

    SetCursorPosition((BOARD_X) + x * 2, BOARD_Y + BOARD_HEIGHT); //ì½˜ì†”ì¢Œí‘œ

    printf("â”");

  }

  //ì™¼ìª½ ë³´ë“œ ë¼ì¸

  for (y = 0; y &lt; BOARD_HEIGHT + 1; y++)

  {

    board[y][0] = 1; //board ë°°ì—´ ì™¼ìª½ 1ì¸ì‹

    SetCursorPosition(BOARD_X, BOARD_Y + y);

    if (y == BOARD_HEIGHT)

      printf("â”—");

    else

      printf("â”ƒ");

  }

  //ì˜¤ë¥¸ìª½ ë³´ë“œ ë¼ì¸

  for (y = 0; y &lt; BOARD_HEIGHT + 1; y++)

  {

    board[y][BOARD_WIDTH + 1] = 1; //board ë°°ì—´ ì˜¤ë¥¸ìª½ 1ì¸ì‹

    SetCursorPosition(BOARD_X + (BOARD_WIDTH + 2) * 2, BOARD_Y + y);

    if (y == BOARD_HEIGHT)

      printf("â”›");

    else

      printf("â”ƒ");

  }

  //ëª¨ì„œë¦¬ê°’ ë³€ê²½

  board[20][0] = 1;

  board[20][11] = 1;

  puts(" ");

}

// ì ìˆ˜íŒ ì¶œë ¥

void ShowScore(void)

{

  SetCursorPosition(40, 3);

  printf("Cì–¸ì–´ ì½˜ì†” í…ŒìŠ¤ë¦¬ìŠ¤");

  SetCursorPosition(40, 5);

  printf("ë ˆë²¨: %d\n", level);

  SetCursorPosition(40, 7);

  printf("ì ìˆ˜: %d\n", score);

}

// ì‚¬ìš© ê°€ëŠ¥ ìœ„ì¹˜ ì²´í¬

// íŠ¹ì • ìœ„ì¹˜ì— ë¸”ë¡ì´ ë“¤ì–´ê°ˆ ìˆ˜ ìˆëŠ”ì§€ ì•„ë‹Œì§€ë¥¼ ì²´í¬

// ë“¤ì–´ê°ˆ ìˆ˜ ìˆìœ¼ë©´ true, ì—†ìœ¼ë©´ false ë°˜í™˜

bool CanPositionedAt(int rotation, int move1, int move2)

{

  int x, y;

  int arrX, arrY; // ë°°ì—´ì¢Œí‘œì €ì¥

  COORD pos = GetCursorPosition();

  arrX = pos.X + move1;

  arrY = pos.Y + move2;

  arrX = (arrX / 2) - 2;

  arrY = arrY - BOARD_Y;

  for (y = 0; y &lt; 4; y++)

  {

    for (x = 0; x &lt; 4; x++)

    {

      if ((blocks[rotation][y][x] == 1) &amp;&amp; board[arrY + y][arrX + x] == 1)

        return false; //ë³´ë“œì™€ ë²½ëŒ ê²¹ì¹¨

    }

  }

  return true; //ê²¹ì¹˜ì§€ ì•ŠìŒ

}

// í˜„ì¬ ìœ„ì¹˜ì— ë¸”ë¡ ì¶œë ¥

void WriteBlock(int rotation)

{

  int i, j;

  COORD cursor = GetCursorPosition();

  if (CanPositionedAt(rotation, 0, 0) == true)

  {

    //ì½˜ì†”ì°½ìœ„ì¹˜ ì„¤ì •, ë°°ì—´ê°’ì—ì„œ 1ì€ â– ì¶œë ¥, 0ì€ ì¶œë ¥ì—†ìŒ

    for (i = 0; i &lt; 4; i++) // í–‰ ë°˜ë³µ

    {

      for (j = 0; j &lt; 4; j++) // ì—´ ë°˜ë³µ

      {

        SetCursorPosition(cursor.X + (j * 2), cursor.Y + i);

        if (blocks[rotation][i][j] == 1)

        {

          printf("â– ");

        }

      }

    }

    SetCursorPosition(cursor.X, cursor.Y);

  }

}

// ë¸”ë¡ ë³´ë“œíŒì—ì„œ 1ì¸ì‹

void BoardInit(int n, int move1, int move2)

{

  COORD pos = GetCursorPosition();

  int arrX = pos.X + move1; //ì½˜ì†”ì¢Œí‘œ ì—´

  int arrY = pos.Y + move2; //ì½˜ì†”ì¢Œí‘œ í–‰

  int x, y;

  /*ì»¤ì„œìœ„ì¹˜ì •ë³´-&gt;ë°°ì—´ìœ„ì¹˜ì •ë³´ ë³€ê²½*/

  arrX = arrX / 2 - 2; //ì½˜ì†”ì¢Œí‘œ-&gt;ë°°ì—´ ì—´ ë³€í™˜ê°’

  arrY = arrY - 2; //ì½˜ì†”ì¢Œí‘œ-&gt;ë°°ì—´ í–‰ ë³€í™˜ê°’

  //ë³´ë“œíŒì—ì„œ ë¸”ë¡ ì´ë™ì‹œ 1ì¸ì‹

  for (y = 0; y &lt; 4; y++)

  {

    for (x = 0; x &lt; 4; x++)

    {

      if (blocks[n][y][x] == 1)

      {

        board[arrY + y][arrX + x] = 1;

      }

    }

  }

}

// ë°°ì—´,ë¸”ë¡ ì˜®ê¹€

void Stepper(int column)

{

  int y, x;

  // boardë°°ì—´ ê°’ í–‰ ë‹¤ìš´

  for (y = column; y &gt;= 0; y--)

  {

    for (x = 1; x &lt;= 10; x++)

    {

      board[y][x] = board[y - 1][x];

    }

  }

  // boardë°°ì—´ 0í–‰ì— 0ì‚½ì…

  for (x = 1; x &lt;= 10; x++)

    board[0][x] = 0;

  // boardë°°ì—´ 1ê°’ ì „ì²´ ì¶œë ¥

  for (y = 1; y &lt;= 19; y++)

  {

    for (x = 1; x &lt;= 10; x++)

    {

      SetCursorPosition((BOARD_X) + x * 2 + 1, y + BOARD_Y);

      if (board[y][x] == 1)

        printf("â– ");

      else

        printf(" ");

    }

  }

}

//[ ë ˆë²¨ ìŠ¤ì½”ì–´ ê³„ì‚°

void CountScore(void)

{

  score += 10;

  if (score % 50 == 0)

  {

    level += 1;

    speed -= 10; // ë ˆë²¨ 1ì¦ê°€ì‹œ, 10 ë°€ë¦¬ì´ˆì”© ë¹¨ë¼ì§

  }

  ShowScore();

}

//[12]

// 1~10ê¹Œì§€ í–‰ì˜ ì—´ ì „ì²´ê°€ 1ë¡œë˜ë©´ ë¸”ë¡ì‚¬ë¼ì§. ì‚¬ë¼ì§€ë©´ Stepperí•¨ìˆ˜ ì‹¤í–‰

void RemoveLine(void)

{

  int i;

  int x, y;

  int z = 0;

  // 19í–‰ë¶€í„° ì‹œì‘í•´ì„œ 1í–‰ê¹Œì§€ ë°˜ë³µ

  for (y = 19; y &gt;= 1; y--)

  {

    //í–‰ê¸°ì¤€ìœ¼ë¡œ 4ë²ˆ ë°˜ë³µ

    for (z = 0; z &lt; 4; z++)

    {

      i = 0;

      //1ì—´ë¶€í„° 10ì—´ê¹Œì§€ ì¦ê°€

      for (x = 1; x &lt; 11; x++)

      {

        //í–‰ê¸°ì¤€

        if (board[y][x] == 1)

        {

          i++;

          //1ì´ 10ê°œë©´ í–‰ ë¸”ë¡ ì‚­ì œ

          if (i == 10)

          {

            for (x = 1; x &lt; 11; x++)

            {

              SetCursorPosition((x + 2) * 2, y + 2);

              printf(" ");

            }

            //í–‰ ê¸°ì¤€ìœ¼ë¡œ ë¸”ë¡ ë‚´ë¦¬ê¸°

            CountScore();

            Stepper(y);

          }

        }

      }

    }

  }

}

// í˜„ì¬ ë¸”ë¡ í´ë¦¬ì–´

void ClearBlock(int rotation, int move1, int move2)

{

  int x, y;

  COORD cursor = GetCursorPosition();

  if (CanPositionedAt(rotation, move1, move2) == true)

  {

    for (y = 0; y &lt; 4; y++)

    {

      for (x = 0; x &lt; 4; x++)

      {

        SetCursorPosition(cursor.X + (x * 2), cursor.Y + y);

        if (blocks[rotation][y][x] == 1)

          printf(" ");

      }

    }

    SetCursorPosition(cursor.X + move1, cursor.Y + move2);

  }

}

// ê²Œì„ ì‹œì‘

void StartGame(void)

{

  int n; // ë¸”ë¡ì¢…ë¥˜ë¥¼ ê²°ì •í•˜ëŠ” ë³€ìˆ˜

  int kb; // í‚¤ë³´ë“œ ê°’

  int c = 2;

  srand((unsigned) time(0)); // rand() í•¨ìˆ˜ë¡œ ëœë¤ê°’ì„ ì£¼ê¸° ìœ„í•´ì„œ ì´ˆê¸°ê°’ ë¶€ì—¬

  PlaySound(TEXT("tetris.wav"), NULL, SND_ASYNC | SND_LOOP);

  // ê²Œì„ ì‹œì‘~ë

  while (1)

  {

    //ë¸”ë¡ ìƒì„± ìœ„ì¹˜ ì¢Œí‘œ(13, 2)ì—ì„œ ì‹œì‘

    SetCursorPosition(13, 2);

    n = rand() % 7; // 0~27ê¹Œì§€ì˜ ì¸ë±ìŠ¤ ìƒì„± : ë¸”ë¡ ì¢…ë¥˜ ê²°ì •

    n = n * 4; // ê° ë¸”ë¡ì˜ ì²«ë²ˆì§¸ ë¸”ë¡(0, 4, 8, 12, 16, 20, 24)ì„ ê¸°ì¤€ìœ¼ë¡œ ì¶œë ¥ ê·¸ë¦¬ê³  ë°©í–¥í‚¤ë¡œ ë³€í™˜í•  ìˆ˜
    ìˆë„ë¡

    if (level == 30) // ë ˆë²¨ì´ 30 ì— ë„ë‹¬í•˜ë©´ ê²Œì„ ë

    {

      SetCursorPosition(40, 15);

      printf("Game Clear");

      getchar();

      exit(1);

    }

    if (CanPositionedAt(n, 0, 0) == false)

      break; //ê²Œì„ ë

    // ë¸”ë¡ í•œê°œ ìœ„~ë°‘ ì´ë™

    while (1)

    {

      int ww = 0;

      int k = 0;

      // ë¸”ë¡ ì•„ë˜ë¡œ ì´ë™

      while (!_kbhit())

      {

        //ë¸”ë¡ ì‡¼

        WriteBlock(n);

        //ë”œë ˆì´ íƒ€ì„

        Sleep(DELAY + speed);

        //ì•„ë˜ì´ë™ì‹œ 1ìˆëŠ”ì§€ í™•ì¸

        if (CanPositionedAt(n, 0, 1) == false)

        {

          ww = 1;

          BoardInit(n, 0, 0); //ë³´ë“œ ë²½ëŒ ë°°ì—´ 1ì¶”ê°€

          RemoveLine();

          break;

        }

        ClearBlock(n, 0, 1); //boardë°°ì—´ +1í–‰

      }

      // CanPositionedAtí•¨ìˆ˜ì—ì„œ ë°°ì—´ê°’ 1ë°œê²¬ì‹œ ì¤‘ì§€

      if (ww == 1)

        break;

      kb = _getch();

      // ë°©í–¥í‚¤

      switch (kb)

      {

      case LEFT:

        ClearBlock(n, -2, 0);

        WriteBlock(n);

        break;

      case RIGHT:

        ClearBlock(n, 2, 0);

        WriteBlock(n);

        break;

      case UP:

        // ì²«ìˆ˜ë¥¼êµ¬í•œë‹¤.

        k = n / 4;

        k *= 4;

        // ë‹¤ìŒìˆ˜ê°€ ëìˆ˜ì´í•˜ì¸ê°€?

        if ((n + 1) &lt;= (k + 3))

        {

          k = n + 1;

        }

        if (CanPositionedAt(k, 0, 0) == true)

        {

          ClearBlock(n, 0, 0);

          n = k;

          WriteBlock(n);

          break;

        }

        break;

      case DOWN:

        ClearBlock(n, 0, 2);

        break;

      case SPACE: // ì•„ë˜ë¡œ ëš ë–¨ì–´ì§€ëŠ” ë¡œì§

        while (1)

        {

          ClearBlock(n, 0, 1);

          if (CanPositionedAt(n, 0, 1) == false)

          {

            WriteBlock(n);

            BoardInit(n, 0, 0);

            break;

          }

        }

      default:
        break;

      } // end switch

    } // end while(ë¸”ë¡)

  } // end while(ê²Œì„)

}

// ê²Œì„ ì¢…ë£Œ í™”ë©´

void EndGame()

{

  SetCursorPosition(40, 15);

  printf("Game Over");

  getch(); // ì…ë ¥ ëŒ€ê¸°

  system("cls");

}

// ë©”ì¸ í•¨ìˆ˜

int main()

{

  ConsoleInit(); // ì‹œì‘ í™”ë©´

  DrawField(); // ê²Œì„ ì˜ì—­ ì¶œë ¥

  ShowScore(); // ì ìˆ˜íŒ ì¶œë ¥

  StartGame(); // ê²Œì„ ì‹œì‘

  EndGame(); // ê²Œì„ ì¢…ë£Œ í™”ë©´

}
